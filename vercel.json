{
  "version": 2,
  "builds": [
    {
      "src": "dist",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "prisma generate && npm run build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "@database-url"
    }
  }
}
